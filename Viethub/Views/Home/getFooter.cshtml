@model IEnumerable<Viethub.Models.Footer>
<!-- footer -->
@{ Html.RenderAction("getOtherInfo", "Home");}
<!-- end footer -->
<!-- copyright -->
<div class="copyright">
    <div class="container">
        <div class="row">
            <div class="col-lg-6 col-md-12">
                <p>
                    Copyrights &copy; 2019 - <a href="https://imransdesign.com/">Imran Hossain</a>,  All Rights Reserved.<br>
                    Distributed By - <a href="https://themewagon.com/">Themewagon</a>
                </p>
            </div>
            <div class="col-lg-6 text-right col-md-12">
                <div class="social-icons">
                    <ul>
                        @foreach (var i in Model)
                        {
                            <li><a href="@i.link" target="_blank"><i class="fab @i.icon"></i></a></li>

                        }
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>